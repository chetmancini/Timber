<config>
  <service>
    <listen ip="127.0.0.1:8080"></listen>
    <group name="timbergroup" scheduler="random" enable="true">
      <host name="host1" ip="127.0.0.1:8081" weight="5"></host>
      <host name="host2" ip="127.0.0.1:8082" weight="5"></host>
      <host name="host3" ip="127.0.0.1:8083" weight="5"></host>
    </group>
  </service>

  <admin>
    <web listen="host:port" enable="True" secure="False" refresh="100"></web>
    <ssh listen="host:port" enable="True"></ssh>
    <user name="chet" password="" access="full"></user><user></user>
  </admin>

  <logging file="logfile.log"></logging>

  <manager hostCheckInterval="61">
    <hostCheck interval="71"></hostCheck>
    <configCheck interval="81"></configCheck>
    <heartbeat interval="50"></heartbeat>
  </manager>

</config>